{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Variant": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "TicketType.Microservice.Template"
    }
  },
  "AWS": {
    "Region": "us-east-1",
    "ServiceURL": "http://localhost:4566",
    "UseHttp": true
  },
  "SQS": {
    "QueueUrl": "http://localhost:4566/000000000000/entity-queue"
  },
  "SNS": {
    "EntityTopicUrl": "http://localhost:4566/000000000000/entity-topic",
    "TicketTopicUrl": "http://localhost:4566/000000000000/tickets-topic"
  },
  "LaunchDarkly": {
    "Key": "",
    "UserName": "ticket-type-prototype-dev"
  },
  "Publisher": {
    "Topics": {
      "ticketing": "arn:aws:sns:us-east-1:000000000000:tickets-topic"
    }
  }
}
